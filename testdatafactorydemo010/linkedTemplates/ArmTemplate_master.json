{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testdatafactorydemo010"
		},
		"LS_ADLSConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSConnection'"
		},
		"LS_AdventureWorksLTConnect_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AdventureWorksLTConnect'"
		},
		"LS_BlobConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobConnection'"
		},
		"LS_SQLConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLConnection'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_ADLSConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen27demo.dfs.core.windows.net/"
		},
		"TR_EventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0b09d9a2-10b0-4bdb-966c-fa3ebfa3ba0a/resourceGroups/TestRG002/providers/Microsoft.Storage/storageAccounts/blobstorage7demo"
		},
		"AzureSqlDatabase_PE_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/0b09d9a2-10b0-4bdb-966c-fa3ebfa3ba0a/resourceGroups/TestResourceGroup002/providers/Microsoft.Sql/servers/testsqlserverdemo009"
		},
		"AzureSqlDatabase_PE_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"AzureSqlDatabase_PE_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"testsqlserverdemo009.database.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testdatafactorydemo010_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSConnection_accountKey": {
						"value": "[parameters('LS_ADLSConnection_accountKey')]"
					},
					"LS_AdventureWorksLTConnect_connectionString": {
						"value": "[parameters('LS_AdventureWorksLTConnect_connectionString')]"
					},
					"LS_BlobConnection_connectionString": {
						"value": "[parameters('LS_BlobConnection_connectionString')]"
					},
					"LS_SQLConnection_connectionString": {
						"value": "[parameters('LS_SQLConnection_connectionString')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"LS_ADLSConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSConnection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "testdatafactorydemo010_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_PE_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabase_PE_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabase_PE_properties_groupId": {
						"value": "[parameters('AzureSqlDatabase_PE_properties_groupId')]"
					},
					"AzureSqlDatabase_PE_properties_fqdns": {
						"value": "[parameters('AzureSqlDatabase_PE_properties_fqdns')]"
					},
					"TR_EventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('TR_EventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testdatafactorydemo010_ArmTemplate_0')]"
			]
		}
	]
}